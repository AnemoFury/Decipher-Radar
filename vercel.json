{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend/landing.html"
    },
    {
      "source": "/(index|console|events|rules|pricing|auth|waitlist|radar)",
      "destination": "/frontend/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}
